{
  "name": "@camunda/feel-builtins",
  "version": "0.0.1",
  "description": "Collection of builtin Camunda extensions for FEEL",
  "scripts": {
    "all": "run-s lint test build",
    "lint": "eslint .",
    "test": "karma start",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:watch": "npm run build -- -w",
    "prepare": "run-s build",
    "builtins": "run-s pull:camunda-docs compile:builtins",
    "pull:camunda-docs": "git -C camunda-docs pull || git clone git@github.com:camunda/camunda-docs.git camunda-docs",
    "compile:builtins": "node scripts/compileBuiltins.js",
    "start": "cross-env SINGLE_START=true npm run dev",
    "start:camunda": "cross-env SINGLE_START=camunda npm run dev",
    "dev": "npm test -- --auto-watch --no-single-run"
  },
  "dependencies": {
    "min-dom": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-loader": "^9.2.1",
    "babel-plugin-istanbul": "^7.0.0",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "eslint": "^8.57.0",
    "glob": "^11.0.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-debug-launcher": "^0.0.5",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.1",
    "karma": "^6.4.4",
    "marked": "^16.0.0",
    "mocha-test-container-support": "^0.2.0",
    "mocha": "^10.8.2",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^23.1.1",
    "rollup": "^4.44.2",
    "sinon-chai": "^3.7.0",
    "sinon": "^17.0.1",
    "webpack": "^5.97.1"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camunda/feel-builtins.git"
  },
  "author": "Camunda <https://camunda.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/camunda/feel-builtins/issues"
  },
  "homepage": "https://github.com/camunda/feel-builtins#readme"
}
