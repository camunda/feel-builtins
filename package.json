{
  "name": "@camunda/feel-builtins",
  "version": "0.0.1",
  "description": "Collection of builtin Camunda extensions for FEEL",
  "scripts": {
    "all": "run-s lint build",
    "lint": "eslint .",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:watch": "npm run build -- -w",
    "prepare": "run-s build",
    "builtins": "run-s pull:camunda-docs compile:builtins",
    "pull:camunda-docs": "git -C camunda-docs pull || git clone git@github.com:camunda/camunda-docs.git camunda-docs",
    "compile:builtins": "node scripts/compileBuiltins.js"
  },
  "dependencies": {
    "min-dom": "^4.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "eslint-plugin-bpmn-io": "^2.2.0",
    "eslint": "^9.31.0",
    "glob": "^11.0.3",
    "marked": "^16.0.0",
    "npm-run-all2": "^8.0.0",
    "rollup": "^4.45.0"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camunda/feel-builtins.git"
  },
  "author": "Camunda <https://camunda.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/camunda/feel-builtins/issues"
  },
  "homepage": "https://github.com/camunda/feel-builtins#readme"
}
